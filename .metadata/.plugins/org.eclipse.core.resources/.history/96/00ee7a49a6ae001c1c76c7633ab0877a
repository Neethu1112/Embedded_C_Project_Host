#include<stdio.h>
int main()
{
	static int i=5;
	if (--i){
	        printf("%d ", i);
	        main(5);
	    }
	// Line Splicing

	printf("\nHello GFG\n");
	printf("welcome\n");
	/* Example 2 - both of the below lines will be printed*/ \
	printf("Hello\t");
	printf("World");
	return (0);
}
