#include <stdio.h>

int main()
{
	int age;
	printf("Enter your age = ");
	scanf("%d",&age);
	char *classification;
	classification =( (age <19) ? 'kid' : ((age<65)?  'adult': 'geezer'));

	printf("you are a %s\n",classification);
	return(0);
}

